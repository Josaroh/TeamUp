<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TeamUp.Views.UtilisateurPage">
    <ContentPage.Content>
        <StackLayout>
            <ListView x:Name="listViewUtilisateur">
                <ListView.ItemTemplate>
                    <DataTemplate>
                        <TextCell Text="{Binding id}" Detail="{Binding identifiant}"/>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>